{
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "env": {
    "node": true
  },
  "globals": {
    "document": true,
    "window": true,
    "MouseEvent": true
  },
  "plugins": [],
  "rules": {
    "camelcase": 0,
    "max-nested-callbacks": [2, {
      "maximum": 10
    }],
    "strict": [2, "safe"],
    "one-var": 0,
    "max-len": 0,
    "object-curly-spacing": 0,
    "no-else-return": 0,
    "no-trailing-spaces": 0,
    "no-negated-condition": 0,
    "newline-per-chained-call": [2, {
      "ignoreChainWithDepth": 2
    }],
    "space-before-function-paren": 0,
    "space-before-blocks": [2, {
      "functions": "always"
    }],
    "eol-last": 2,
    "quotes": [
      2,
      "single"
    ],
    "require-jsdoc": 2
  }
}
